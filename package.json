{
  "name": "plick-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "tw:generate":"npx tailwindcss -i ./input.css -o ./src/index.css",
    "tw:watch":"npx tailwindcss -i ./input.css -o ./src/index.css --watch",
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview --host",
    "show":" npm run tw:generate & npm run build && npm run preview"
  },
  "dependencies": {
    "framer-motion": "^8.5.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
